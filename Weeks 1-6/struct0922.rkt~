; A Position is a (make-posn Real Real)
; Interpretation: the x- and y-coordinates of an object

(define POSN-0 (make-posn 0 0))
(define POSN-1 (make-posn 3 4))
(define POSN-2 (make-posn 89.2 pi))

(define (posn-temp p)
  (...
   (posn-x p) ...
   (posn-y p) ...))

